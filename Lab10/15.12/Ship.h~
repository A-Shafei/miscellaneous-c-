#ifndef SHIP_H
#define SHIP_H
#include <string>

class Ship
{
 private:
  std::string name;
  std::string yearBuilt;

 public:
  Ship()
    {
      name = "None given";
      yearBuilt = "None given";
    }
  Ship(std::string sName, std::string sYearBuilt)
    {
      name = sName;
      yearBuilt = sYearBuilt;
    }
  setName(sName)
    {
       name = sName;
    }
  getName()
    {
      return name;
    }
  setYearBuilt(sYearBuilt)
    {
      yearBuilt = sYearBuilt;
    }
  getYearBuilt()
    {
      return yearBuilt;
    }
};

#endif
